    if (window.location.pathname.match(regex)) {
      window.location.replace("/blog/")
    }
  }

  // Redirect missing resources to /resources home (MKT-2115)
  function resourcesRedirect(){
    let regex = /\/resources\/.+/ // match anything under "/resources" subpath
    if (window.location.pathname.match(regex)) {
      window.location.replace("/resources/")
    }
  }

  (function(){
    blogRedirect()
