<!-- Load cookie consent banner -->
<script type="text/javascript">
window.dataLayer = window.dataLayer || []; 
window.hs = window.hs || {}; 
const script = document.createElement("script"); 
script.src= 'https://www.dropbox.com/pithos/privacy_consent'; 
script.async=true; 
document.head.appendChild(script);

const EmbeddableConfig = { 
  injectToFooter: true, 
  injectFloatingButton: true, 
  disableBanner: false, 
  thirdPartyConfig: { 
    cookieDomain: 'hellosign.com' 
  }, 
  consentChangedCallback: (categories) => consentChangedCallback(categories), 
  priorConsentCallback: (categories) => priorConsentCallback(categories), 
  consentDeclinedCallback: (categories) => consentDeclinedCallback(categories), 

 onBannerVisible: (height) => onBannerVisible (height),
 onBannerNotVisible: () => onBannerNotVisible ()
}; 
  window.dropbox = window.dropbox || []; 
  window.dropbox.privacyConsent = window.dropbox.privacyConsent || []; 
  window.dropbox.privacyConsent.autoInit = EmbeddableConfig; 
  
  function stringCategories(categories) {
    const allowedCategories = []; 
    for (const [key, value] of Object.entries(categories)) { 
      if (value) { allowedCategories.push(key); } 
    }
    return allowedCategories.join(',');
  }
  
  document.onreadystatechange = () => { 
    if (document.readyState === 'interactive') { 
      window.dataLayer.push({ 'consent_enabled': true }); 
      //utag.link({ 'consent_enabled': true }); 
    } }
  function consentChangedCallback(categories) { 
    //console.log('Consent Changed: ', categories); 
    gtmCategories(categories); 
  }
  
  function priorConsentCallback(categories) { 
    //console.log("::in priorConsentCallback::", categories); 
    gtmCategories(categories); 
  }
  function onBannerVisible(height) {
    $('._2022-footer').css('padding-bottom', height);
    //mobile
     if($(window).width() <= 500) {
    	$('#designstudio-minimize').css('padding-bottom', height);
     }
     else {
      $('#designstudio-button, #designstudio-iframe,#designstudio-minimize').css('bottom', height);
    }
  }
  function onBannerNotVisible () {
    $('._2022-footer').css('padding-bottom','0px');
    $('#designstudio-button, #designstudio, #designstudio-iframe, #designstudio-minimize').css('bottom', '0px!important');
    $('#designstudio-minimize').css('padding-bottom', '0px!important');

  }

  function gtmCategories(categories) {
    var cookieCategories = ''; 
    var updatedCategories = categories;
     if (!!updatedCategories) { 
       cookieCategories = stringCategories(updatedCategories) 
      }
    //console.log("GTM CATEGORIES:", cookieCategories); 
    window.dataLayer.push({ 'event': 'consentChangedCallback', 'consent_categories': cookieCategories }); 
    window.hs.consentCategories = updatedCategories;
    //for Tealium
	utag.link({ 'event': 'consentChangedCallback', 'consent_categories': cookieCategories });
    
  }
</script>
