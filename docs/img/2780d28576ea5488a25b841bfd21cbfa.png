<!-- Auto-assign da- attributes (MKT-1418) -->
<script>
  $("a").each((i, link) => {
    let el = $(link)[0];
    let href = $(el).attr("href");
    let isButton = [...el.classList].find((className) =>
      className.includes("button")
    )
      ? true
      : false;

    let path = window.location.pathname.toLowerCase();
    let linkText = $(el).text().toLowerCase();
    let isContact = linkText.includes("contact");

    let signupConditions = [
      "try",
      "trial",
      "sign",
      "free",
      "started",
      "create",
    ];
    let isSignup =
      signupConditions.some((el) => linkText.includes(el)) && isButton;
    let isNavSignup = $(el).hasClass("top-nav-cta");

    // Set da-category
    if (!$(el).is("[da-category]")) {
      $(el).attr(
        "da-category",
        isSignup || isNavSignup
          ? "signup"
          : isContact
          ? "contactus-form-start"
          : isButton
          ? "buttonlink-click"
          : "textlink-click"
      );
    }

    // Set da-action & href
    if (!$(el).is("[da-action]")) {
      if (isSignup || isNavSignup) {
        if (path.includes("hellosign")) {
          $(el).attr("href", "https://app.hellosign.com/account/signUp");
          $(el).attr("da-action", "create account start - hs");
        } else if (path.includes("api") || path.includes("developers")) {
          // "BUGREP-5354"
          $(el).attr("href", "https://app.hellosign.com/api/pricing");
          $(el).attr("da-action", window.location.pathname);
        } else if (path.includes("helloworks")) {
          $(el).attr(
            "href",
            "https://app.hellosign.com/account/signUp?on_login_redirect_url=https://portal.helloworks.com/?"
          );
          $(el).attr("da-action", "create account start - hw");
        } else if (path.includes("hellofax")) {
          $(el).attr(
            "href",
            "https://app.hellofax.com/info/pricing?utm_source=hellosign&utm_medium=productshf&utm_campaign=tryhffree"
          );
          $(el).attr("da-action", window.location.pathname);
        } else {
          $(el).attr("href", "https://app.hellosign.com/account/signUp");
          $(el).attr("da-action", "create account start - hs");
        }
      } else {
        $(el).attr("da-action", window.location.pathname);
      }
    }

    // Set da-label
    if (!$(el).is("[da-label]")) {
      if (!href) {
        // link is not set
        $(el).attr("da-label", undefined);
      } else if (isNavSignup) {
        $(el).attr("da-label", "nav bar");
      } else if (href.indexOf("www.hellosign.com") != -1) {
        // root is www.hellosign.com
        $(el).attr("da-label", href.split("www.hellosign.com")[1]);
      } else if (href.indexOf("http") != -1) {
        // root is other website/subdomain
        $(el).attr("da-label", href.split("//")[1]);
      } else {
        // link is other value
        $(el).attr("da-label", href);
      }
    }
  });
</script>
<!-- End da- attributes -->

