background-image: -webkit-gradient(linear,left top,left bottom,from(hsla(0,0%,100%,0)),to(#fff));
background-image: linear-gradient(hsla(0,100%,100%,0),#fff);
bottom: 0px;
content: "";
height: 5.5em;
left: 0;
position: absolute;
right: 0;
width: 100%;

}

