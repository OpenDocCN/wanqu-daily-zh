.overlay {
  position: relative;
}

.overlay:after {
  position: absolute;
  content:"";
  top:0;
  left:0;
  width:100%;
  height:100%;
  opacity:0;
  opacity: .1;
}


.post-preview {
overflow: hidden;
height: 240px;
}


.dark:after {
  background-color: #030303;
}

