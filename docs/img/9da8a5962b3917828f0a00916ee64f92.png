<div id="cookie-consent" class="cookie-consent vanilla-module" data-component="cookie-consent"></div>

    

    
<script>
function handleCanvas(canvas) {
	    document.querySelector('a[href*="#cookie-settings"]').addEventListener("click", function(){ event.preventDefault();cookieConsent.toggle(true, false, true, true)});
}
var observer = new MutationObserver(function (mutations, me) {
  var canvas = document.querySelector(".primary-footer__bottom-links");
  if (canvas) {
    handleCanvas(canvas);
    me.disconnect();
    return;
  }
});
observer.observe(document, {
  childList: true,
  subtree: true
});
var glossary = document.querySelector(".glossaryPage .container .col-lg-6.offset-lg-4")
if (typeof(glossary) != 'undefined' && glossary != null)
{
 glossary.className = "col-14 col-lg-8 offset-lg-3";
}
var stopReferrer = false; 

function setUTMCookie(name,value) { 
        var value = value.replace('https://','');
	var expires = "";
	var date = new Date();
	date.setTime(date.getTime() + (90*24*60*60*1000));
	expires = "; expires=" + date.toUTCString();
	document.cookie = name + "=" + (value || "")  + expires + "; path=/";
}
function getUTMCookie(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}

let utmQuery = decodeURIComponent(window.location.search.substring(1)), utmVariables = utmQuery.split('&'), ParameterName, i;
  
  const getUTMValue = (inputParameter) => {
	for (i = 0; i < utmVariables.length; i++) {
	  ParameterName = utmVariables[i].split('=');
	  if (ParameterName[0] === inputParameter) {
		return ParameterName[1] === null ? null : ParameterName[1];
	  }
	}
  }
  
  const valueExists = (value) => {
	return (value != null && value != '' && value != undefined)
  }
  
  const utmParams = [
	'utm_source',
	'utm_medium',
	'utm_campaign',
	'utm_content',
	'utm_term',
	'gclid'
  ];
  
  utmParams.forEach(param => {
	var pValue = getUTMValue(param);
  
	if (valueExists(pValue) && param != "gclid") {
	  setUTMCookie(param, pValue);
	  stopReferrer = true;
	}else if (getUTMValue("gclid") && param == "gclid"){
		setUTMCookie("utm_source", "google");
		setUTMCookie("utm_medium", "cpc");
		stopReferrer = true;
	}
	let cValue = getUTMCookie(param);
  	let input = document.querySelector('input[' + param + ']');
  	if (input && valueExists(cValue)) {
		input.value = cValue;
  	}
  });
  
  if (valueExists(document.referrer) && !stopReferrer){
	  var urlParts = document.referrer.split('.') 
	  var site = (urlParts.slice(0).slice(-(urlParts.length === 4 ? 3 : 2))[0])  
	  switch (site) {
		case "google":
	    case "bing":
	    case "duckduckgo": 
		case "yahoo": 
		  setUTMCookie("utm_source", site);
		  setUTMCookie("utm_medium", "organic");
		  break;
		case "twitter":
		case "reddit":
		case "facebook":
		case "instagram":
		case "linkedin":
		case "linkedin.com":
		  setUTMCookie("utm_source", site);
		  setUTMCookie("utm_medium", "organic");
		  break;
		case "marketo":
		  setUTMCookie("utm_source", "marketo");
		  setUTMCookie("utm_medium", "email");
		  break;
		default:
		  if(site != "optimizely"){
		  	setUTMCookie("utm_source", site);
	  	  	setUTMCookie("utm_medium", "referral");
		  }
		  break;
	  }
  }
</script>
